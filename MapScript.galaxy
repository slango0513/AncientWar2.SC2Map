    gf_PreviewerAtBuildingRegionPositionAll();
    gf_PreviewUpdateAll();
    gf_CountUnitEnable(true);
    gf_ReserveUnitCreationEnable(true);
    TriggerWaitForTrigger(gt_CountUnitControl, false);
